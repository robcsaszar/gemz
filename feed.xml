---
layout: 
---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.seo.meta_og_title | xml_escape }}</title>
    <description>{{ site.seo.meta_og_description | xml_escape }}</description>
    <link>{{ site.seo.meta_og_url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.seo.meta_og_url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for shop in site.shops limit:10 %}
      <item>
        <title>{{ shop.title | xml_escape }}</title>
        <description>{{ shop.content | strip_html }}</description>
        <pubDate>{{ shop.date | date_to_rfc822 }}</pubDate>
        <link>{{ shop.url | prepend: site.baseurl | prepend: site.seo.meta_og_url }}</link>
        <image><url>{{ shop.image | prepend: site.baseurl | prepend: site.seo.meta_og_url }}</url></image>
        <guid isPermaLink="true">{{ shop.url | prepend: site.baseurl | prepend: site.seo.meta_og_url }}</guid>
        {% for cat in shop.category %}
        <category>{{ cat | xml_escape }}</category>
        {% endfor %}
      </item>
    {% endfor %}
  </channel>
</rss>
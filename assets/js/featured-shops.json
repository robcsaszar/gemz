---
layout: null
---
const featuredShops = [{
{% assign featured_shops = site.shops | where: 'featured', true %}
{%- for shop in featured_shops -%}
"name": {{ shop.title | prepend: '"' | append: '"' }},
"url": {{ shop.url | prepend: '"' | append: '"' }},
"facebook": {{ shop.facebook | prepend: '"' | append: '"' }},
"instagram": {{ shop.instagram | prepend: '"' | append: '"' }},
"website": {{ shop.website | prepend: '"' | append: '"' }},
"image": {{ shop.image | prepend: '"' | append: '"' }}
}{% unless forloop.last %},{
{% endunless %}
{%- endfor -%}
];